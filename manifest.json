{
    "manifest_version": 2,
    "name": "Block site extension",
    "version": "1.0",
  
    "description": "Blocks websites specified",
  
    "icons": {
      "48": "icons/icon-48.png"
    },

    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/"],
        "js": ["blockSite.js"]
      }
    ]
  }
  